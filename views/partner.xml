<odoo>
    <data>
        <record id="partner_view_partner_form_inherit" model="ir.ui.view">
            <field name="name">partner.view.partner.form.inherit.inherit</field>
            <field name="model">res.partner</field>
            <field name="inherit_id" ref="base.view_partner_form"/>
            <field name="arch" type="xml">
                <xpath expr="//notebook" position="inside">
                    <page string="Products" name="products">
                        <field name="rx_product_line_ids" widget="one2many_list" class="oe_inline oe_no_button">
                            <tree editable="bottom">
                                <field name="rx_product_name"/>
                                <field name="rx_qty"/>
                            </tree>
                        </field>
                    </page>
                </xpath>
            </field>
        </record>
    </data>
</odoo>
